{
  "hosting": {
    "public": "dist/onepay-frontend/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "*/assets/**",
        "destination": "/assets/$1"
      }
    ]
  }
}
